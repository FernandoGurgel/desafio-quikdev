{
   "name": "desafio-quikdev",
   "version": "1.0.0",
   "description": "Esse projeto tem como objetivo mostrar meus conhecimentos com a tecnoliga node e seu framework para a empresa quikdev.",
   "main": "app.js",
   "license": "MIT",
   "scripts": {
      "build": "tsc",
      "dev:server": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/app.ts",
      "start": "ts-node src/index.ts",
      "typeorm:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
      "typeorm:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
      "swagger:genarate": "ts-node src/config/swagger.ts"
   },
   "dependencies": {
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "express": "^4.19.2",
      "express-async-errors": "^3.1.1",
      "jsonwebtoken": "^9.0.2",
      "pg": "^8.0.0",
      "reflect-metadata": "^0.1.13",
      "swagger-autogen": "^2.23.7",
      "swagger-ui-express": "^5.0.0",
      "typeorm": "0.2.45"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.5",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.12",
      "@types/express-serve-static-core": "4.17.2",
      "@types/jsonwebtoken": "^9.0.6",
      "@types/node": "^16.11.10",
      "@types/swagger-ui-express": "^4.1.6",
      "ts-node": "10.4.0",
      "ts-node-dev": "^1.0.0-pre.44",
      "typescript": "^5.4.5"
   }
}
